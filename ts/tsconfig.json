{
    "compilerOptions" : {
        "target" : "es5",
        "module" : "commonjs",
        "outDir" : "./target/build/js",
        "baseUrl" : ".",
        "sourceMap" : false,
        "noImplicitAny" : false,
        "allowJs" : false,
        "declaration" : false,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "lib": [
            "es6",
            "es2017.object"
        ],
        "traceResolution": false
    },
    "include": [
        "./src/**/*.ts",
    ],
    "exclude" : []
}
